{
  "ver": "0.0.1",
  "authors": ["ac.shahnam"],
  "contact": "",
  "categories": ["active"],
  "widgets": { "input": null, "output": "kbaseReportView" },

  "parameters": [
    {
      "id": "reads_refs",
      "optional": false,
      "advanced": false,
      "allow_multiple": true,
      "default_values": [],
      "field_type": "text",
      "text_options": {
        "valid_ws_types": [
          "KBaseFile.SingleEndLibrary",
          "KBaseReads.SingleEndLibrary"
        ]
      }
    },
    {
      "id": "assembly_name",
      "optional": true,
      "advanced": false,
      "allow_multiple": false,
      "default_values": ["Raven.Assembly"],
      "field_type": "text"
    },
    {
      "id": "threads",
      "optional": true,
      "advanced": true,
      "allow_multiple": false,
      "default_values": ["4"],
      "field_type": "text"
    },
    {
      "id": "save_gfa",
      "optional": true,
      "advanced": true,
      "allow_multiple": false,
      "default_values": ["0"],
      "field_type": "checkbox",
      "checkbox_options": { "checked_value": "1", "unchecked_value": "0" }
    }
  ],

  "behavior": {
    "service-mapping": {
      "url": "",
      "name": "kb_raven",
      "method": "run_raven_assembler",
      "input_mapping": [
        { "narrative_system_variable": "workspace",    "target_property": "workspace_name", "target_argument_position": 0 },
        { "narrative_system_variable": "workspace_id", "target_property": "workspace_id",   "target_argument_position": 0 },

        { "input_parameter": "reads_refs",    "target_property": "reads_refs",    "target_argument_position": 0, "target_type_transform": "list<ref>" },
        { "input_parameter": "assembly_name", "target_property": "assembly_name", "target_argument_position": 0 },
        { "input_parameter": "threads",       "target_property": "threads",       "target_argument_position": 0, "target_type_transform": "int" },
        { "input_parameter": "save_gfa",      "target_property": "save_gfa",      "target_argument_position": 0, "target_type_transform": "int" }
      ],
      "output_mapping": [
        { "service_method_output_path": [0, "report_name"], "target_property": "report_name" },
        { "service_method_output_path": [0, "report_ref"],  "target_property": "report_ref"  }
      ]
    }
  },

  "job_id_output_field": "docker"
}
