{
  "ver": "1.3.1",
  "authors": ["ac.shahnam"],
  "contact": "sv1@ornl.gov",
  "categories": ["active", "assembly"],
  "widgets": {
    "input": null,
    "output": "no-display"
  },
  "parameters": [
    {
      "id": "read_libraries",
      "optional": false,
      "advanced": false,
      "allow_multiple": true,
      "default_values": [""],
      "field_type": "text",
      "text_options": {
        "valid_ws_types": ["KBaseFile.SingleEndLibrary", "KBaseAssembly.SingleEndLibrary"]
      }
    },
    {
      "id": "assembly_name",
      "optional": false,
      "advanced": false,
      "allow_multiple": false,
      "default_values": [""],
      "field_type": "text",
      "text_options": {
        "is_output_name": true
      }
    },
    {
      "id": "threads",
      "optional": true,
      "advanced": false,
      "allow_multiple": false,
      "default_values": ["8"],
      "field_type": "text",
      "text_options": { "validate_as": "int", "min_int": 1, "max_int": 64 }
    },
    {
      "id": "polishing_rounds",
      "optional": true,
      "advanced": false,
      "allow_multiple": false,
      "default_values": ["2"],
      "field_type": "text",
      "text_options": { "validate_as": "int", "min_int": 0, "max_int": 16 }
    },
    {
      "id": "kmer_len",
      "optional": true,
      "advanced": true,
      "allow_multiple": false,
      "default_values": ["15"],
      "field_type": "text",
      "text_options": { "validate_as": "int", "min_int": 4, "max_int": 63 }
    },
    {
      "id": "window_len",
      "optional": true,
      "advanced": true,
      "allow_multiple": false,
      "default_values": ["5"],
      "field_type": "text",
      "text_options": { "validate_as": "int", "min_int": 1, "max_int": 100 }
    },
    {
      "id": "frequency",
      "optional": true,
      "advanced": true,
      "allow_multiple": false,
      "default_values": ["0.001"],
      "field_type": "text",
      "text_options": { "validate_as": "float", "min_float": 0, "max_float": 1 }
    },
    {
      "id": "identity",
      "optional": true,
      "advanced": true,
      "allow_multiple": false,
      "default_values": ["0"],
      "field_type": "text",
      "text_options": { "validate_as": "float", "min_float": 0, "max_float": 1 }
    },
    {
      "id": "max_num_overlaps",
      "optional": true,
      "advanced": true,
      "allow_multiple": false,
      "default_values": ["32"],
      "field_type": "text",
      "text_options": { "validate_as": "int", "min_int": 1, "max_int": 10000 }
    },
    {
      "id": "min_unitig_size",
      "optional": true,
      "advanced": true,
      "allow_multiple": false,
      "default_values": ["9999"],
      "field_type": "text",
      "text_options": { "validate_as": "int", "min_int": 0, "max_int": 100000000 }
    },
    {
      "id": "write_gfa",
      "optional": true,
      "advanced": false,
      "allow_multiple": false,
      "default_values": ["0"],
      "field_type": "checkbox",
      "checkbox_options": { "checked_value": "1", "unchecked_value": "0" }
    },
    {
      "id": "resume",
      "optional": true,
      "advanced": true,
      "allow_multiple": false,
      "default_values": ["0"],
      "field_type": "checkbox",
      "checkbox_options": { "checked_value": "1", "unchecked_value": "0" }
    }
  ],
  "behavior": {
    "service-mapping": {
      "url": "",
      "name": "kb_raven",
      "method": "run_kb_raven",
      "input_mapping": [
        { "narrative_system_variable": "workspace", "target_property": "workspace_name" },
        { "input_parameter": "read_libraries", "target_property": "read_libraries", "target_type_transform": "list<ref>" },
        { "input_parameter": "assembly_name", "target_property": "assembly_name" },
        { "input_parameter": "threads", "target_property": "threads", "target_type_transform": "int" },
        { "input_parameter": "polishing_rounds", "target_property": "polishing_rounds", "target_type_transform": "int" },
        { "input_parameter": "kmer_len", "target_property": "kmer_len", "target_type_transform": "int" },
        { "input_parameter": "window_len", "target_property": "window_len", "target_type_transform": "int" },
        { "input_parameter": "frequency", "target_property": "frequency" },
        { "input_parameter": "identity", "target_property": "identity" },
        { "input_parameter": "max_num_overlaps", "target_property": "max_num_overlaps", "target_type_transform": "int" },
        { "input_parameter": "min_unitig_size", "target_property": "min_unitig_size", "target_type_transform": "int" },
        { "input_parameter": "write_gfa", "target_property": "write_gfa", "target_type_transform": "int" },
        { "input_parameter": "resume", "target_property": "resume", "target_type_transform": "int" }
      ],
      "output_mapping": [
        { "service_method_output_path": [0, "report_name"], "target_property": "report_name" },
        { "service_method_output_path": [0, "report_ref"],  "target_property": "report_ref" }
      ]
    }
  },
  "job_id_output_field": "docker"
}
